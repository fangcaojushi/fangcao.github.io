.zs_color{
	display: inline-block;
    width: 30px;
    height: 30px;    
    line-height: 30px;
    text-align: center;
    vertical-align: middle;
    position: relative;
}
.zs_color span{
	width: 100%;
    height: 100%;
    color: #fff;
    border-radius: 20px;
    position: absolute;
}
.zs_color span:first-child{
    opacity: 0.4;
    left: -4px;
    top: -4px;
}
.zs_color span:nth-child(2){
	left: 0;
    top: 0;
}
