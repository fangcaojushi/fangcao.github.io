#dates li {
	width: 100%;
	position: relative;
	height: 80px;
	text-align: right;
	padding-right: 30px;
	cursor: pointer;
	text-indent: 0;
}
#dates li:hover{
	font-weight: bold;
}
#dates li.active a h3 {
	color: #f8860b;
}
#dates li.active i {
	background: #f8860b;
	border:1px double #ea7c04;
	box-shadow: 1px 1px 5px rgba(248,134,11,.8);
}
#dates li i{
	width: 20px;
	height: 20px;
	background: #1AB394;
	position: absolute;
	right: -13px;
	text-align: center;
	line-height: 18px;
	top:20px;
	z-index: 9999;
	display: block;
	border-radius: 10px;
	border:1px double #0B997C;
	color: #fff;
	box-shadow: 1px 1px 5px rgba(20,177,145,.8);
	cursor: pointer;
	font-style: normal;
}
#dates a {
	padding-top: 10px;
}
#dates a h3{
	color: #1AB394;
	padding-top: 10px;
}
#dates a p{
	color: #C0C0C0;
}
#dates .selected {
}

#issues {
	width:75%;
	height: 100%;
	overflow: hidden;
	float: left;
}

