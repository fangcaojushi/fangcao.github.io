/* timeline */
.high100{
	height: 100%;
}
#timeline {
	width: 100%;
	height: 100%;
	overflow: hidden;
	position: relative;
	background: #F3F3F4;
}
.hdetailtitle{
	background: #1AB394;
	color: #fff;
	text-align: center;
	margin: 0;
	padding: 10px 0;
}
#dates {
	position: relative;
	height: 100%;
	width:20%;
	left: auto;
	margin-left: auto;
	bottom: auto;
	z-index: auto;
	padding: 30px 0;
	box-sizing: border-box;
	float: left;
	margin-right:20px;
	border-right: 2px #1AB394 solid;
	background: #fff;
	padding-right: 2px;
	
}
