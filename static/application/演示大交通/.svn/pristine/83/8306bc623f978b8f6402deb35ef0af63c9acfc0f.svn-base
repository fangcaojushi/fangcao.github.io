/******************************标题样式*****************************/

.lg-txt {
	width: 100%;
	position: absolute;
	bottom: 14.5%;
	left: 50%;
	margin-left: -25%;
}

.lg-txt .light {
	position: absolute;
	left: 0px;
	width: 180px;
	height: 101px;
	z-index: 556;
	background-image: -moz-linear-gradient(0deg, rgba(248, 235, 3, 0), rgba(30, 228, 240, 0), rgba(248, 235, 3, 0));
	background-image: -webkit-linear-gradient(0deg, rgba(248, 235, 3, 0), rgba(30, 235, 240, 0), rgba(248, 235, 3, 0));
	-moz-animation: lightword 4s infinite;
	-o-animation: lightword 4s infinite;
	-ms-animation: lightword 4s infinite;
	animation: lightword 4s infinite;
	opacity: 0;
	border-radius: 10px;
}

.lg-txt .light .faguang {
	position: absolute;
	left: 0;
	z-index: 999;
	top: -46px;
	width: 130%;
	height: 100%;
}

@-moz-keyframes lightword {
	0% {
		left: -40px;
		opacity: 0;
	}
	50% {
		opacity: 1.5;
	}
	100% {
		left: 248px;
		opacity: 0
	}
}

@-o-keyframes lightword {
	0% {
		left: -40px;
		opacity: 0;
	}
	50% {
		opacity: 1.5;
	}
	100% {
		left: 248px;
		opacity: 0
	}
}

@-ms-keyframes lightword {
	0% {
		left: -40px;
		opacity: 0;
	}
	50% {
		opacity: 1.5;
	}
	100% {
		left: 248px;
		opacity: 0
	}
}

@keyframes lightword {
	0% {
		left: -40px;
		opacity: 0;
	}
	50% {
		opacity: 1.5;
	}
	100% {
		left: 248px;
		opacity: 0
	}
}

.fen-title {
	z-index: 999;
	background: url(../images/title.png) no-repeat;
	background-position: center center;
	margin-bottom: 0.5%;
	margin-top: 0.5%;
}

.fen-title h2 {
	margin-top: 11px;
	margin-bottom: 13px;
	font-size: 22px;
	/*font-weight: bold;*/
	color: #fff;
	opacity: 0.8;
	font-family: '微软雅黑';
	/*letter-spacing:2px;*/
	/*background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(rgb(0, 255, 0)), to(rgb(35,127,56)));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;*/
}

@media only screen and (max-width:1440px) {
	.lg-txt .light .faguang {
		position: absolute;
		left: 0;
		z-index: 999;
		top: -46px;
		width: 75%;
	}
}

@media only screen and (max-width:1024px) {
