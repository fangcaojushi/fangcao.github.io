@charset "UTF-8";

/* CSS Document */

body,
html,
#allmap {
	width: 100%;
	height: 100%;
	overflow: hidden;
	margin: 0;
	font-family: "微软雅黑";
	color: #aabad5;
	font-size: 12px;
}

body {
	background: url(../images/bg.jpg) no-repeat/*rgba(0,16,31,1)*/
	;
	background-size: cover;
	background-color: #0C1732;
}

a,
a:hover,
a:active,
a:link {
	text-decoration: none;
}

a {
	outline: none;
	blr: expression(this.onFocus=this.blur());
}

a:focus {
	outline: none
}

* {
	margin: 0;
	padding: 0;
}

ul,
li {
	list-style: none;
}

iframe {
	border: 0;
}

