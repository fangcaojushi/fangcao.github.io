.title_sub {
	position: absolute;
	left: 50%;
	top: 0;
	/*background:rgba(12, 24, 50, .7);*/
	text-align: center;
	padding: 10px;
	width: 50%;
	margin-left: -25%;
	z-index: 9999;
	transition: all .5s;
	border-radius: 5px;
}

@media only screen and (max-width:1366px) {
	.gj_title {
		font-size: 20px;
		margin: 10px;
		font-weight: bold;
	}
}

.ysmenu {
	height: 35px;
	line-height: 35px;
	background: rgba(5, 45, 192, 0.5);
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 99999;
	text-align: center
}

.ysmenu a {
	font-size: 16px;
	height: 35px;
	line-height: 35px;
	padding: 0 10px;
	color: #CED7F8;
	display: inline-block;
}

.ysmenu a:hover {
	color: #fff;
	background: rgba(0, 255, 110, 0.60);
}

.fen {
	clear: both;
	height: 20px;
}


/*****************************右侧收缩窗**********************************************************/

.shoushuo_window {
	width: 300px;
	z-index: 9999;
	position: absolute;
	right: 0;
	transition: right 300ms;
}

.shoushuo_window1 {
	width: 300px;
	z-index: 9999;
	position: absolute;
	left: 0;
	transition: left 300ms;
}

.win_position1 {
	top: 100px;
	height: 300px
}

.win_position2 {
	top: 430px;
	height: 200px
}

.win_position3 {
	top: 80px;
	height: 220px
}

.win_position4 {
	top: 320px;
	height: 260px
}

.win_position5 {
	top: 80px;
	height: 520px
}

.shoushuo_window_hidden {
	right: -300px;
}

.shoushuo_window1_hidden {
	left: -300px;
}

.shoushuo_window .win_title {
	width: 30px;
	height: auto;
	padding: 10px;
	z-index: 9999;
	position: absolute;
	top: 0px;
	left: -30px;
	cursor: pointer;
	display: block
}

.shoushuo_window1 .win_title1 {
	width: 30px;
	height: auto;
	padding: 10px;
	z-index: 9999;
	position: absolute;
	top: 0px;
	right: -30px;
	cursor: pointer;
	display: block
}


