.search_left{margin-left: auto;margin-right:auto;padding:10px 8px;clear:both;background-color:transparent;border-bottom:dotted 1px #1e3356;}
.left_z{  height: 100%;width:260px;background-color: rgb(21, 30, 45);box-sizing:border-box;}
.search_left .text{vertical-align:middle;width:100%;height:28px;color: #334a6f;border: 1px solid rgba(12, 49, 128, 0.6);display: block;padding:0 30px;background-color:transparent;border-radius:0;font-size:12px;outline: none;}
