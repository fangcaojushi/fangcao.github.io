/*****************************云台右侧分项定位*******************************/
.sp_con li .focus{
    width:115px;
    height:115px;
    position:absolute;
    top:20px;
    right:4px;
    text-align:center;
}
.sp_con li .yzw{
    position: relative;
    font-size:12px;
    width:100%;
    margin:0 auto;
    table-layout:fixed;
    border-collapse: collapse;
}
.sp_con li .yzw tr{
    border-bottom:1px  dotted rgba(5,40,245,0.40);
   
    /*display: inline-block;*/
    line-height:25px;
    height:25px;
    width:100%;
    box-sizing:border-box;
    cursor:default;
}
.sp_con li .yzw tr td{
    text-align: center;
    padding: 3px;
    box-sizing:border-box;
}
.sp_con li .yzw tr td:nth-child(1){
    width:10%;
}
.sp_con li .yzw tr td:nth-child(2){
    width:60%;
}
.sp_con li .yzw tr td:nth-child(3){
    padding-top:5px;
    width:40%;
}
.sp_con li .yzw tr:hover{
    background:#000000;
    opacity: 0.9;
}

.sp_con li .opt{
    width:14px;
    height:14px;
    display: inline-block;
    margin-top:-10px;
    cursor:pointer;
}
.sp_con li .opt-edit{
    background:url(../images/edit.png) no-repeat;
}
.sp_con li .opt-pre{
    background:url(../images/pre.png) no-repeat;
}
.sp_con li .opt-del{
    background:url(../images/del.png) no-repeat;
}

