/*右侧视频插件*/
.right_z{height: 100%; }
.right_z img{ width: 100%;height: 100%;}


/*手风琴效果*/
.accordion_left{background-color:rgb(21, 30, 45);}
.spchange{position: absolute;bottom: 0px; left: 0px;width:260px}
.accordion_left .accordion {
    width: 100%;
    max-width: 360px;
    margin: 0 auto 0;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    position: relative;
}

.accordion_left .accordion .link {
    cursor: pointer;
    display: block;
    color: #4d4d4d;
    font-size: 14px;
    font-weight: 700;
    position: relative;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
}

.accordion_left .accordion li:last-child .link {
    border-bottom: 0;
}

.accordion_left .accordion li i {
    background-color: transparent;
    position: absolute;
    top: 6px;
    left: 12px;
    font-size: 18px;
    color: rgba(9, 36, 193, 0.9);
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
}

.accordion_left .accordion li i.fa-chevron-down {
    right: 12px;
    left: auto;
    font-size: 16px;
}

.accordion_left .accordion li.open i.fa-chevron-down {
    -webkit-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);
}


/*----------------------------云台、预置位-----------------------------*/
.accordion_left .accordion .submenu {
    display: none;
    background-color: transparent;
    font-size: 12px;
    color: #fff;
}
.accordion_left .accordion .submenu td{ color: #5e6e94;}

.accordion_left .accordion .submenu li {
    border-bottom: 1px solid #4b4a5e;
}

.accordion_left .accordion .submenu a {
    display: block;
    text-decoration: none;
    color: #d9d9d9;
    padding: 12px;
    padding-left: 42px;
    -webkit-transition: all 0.25s ease;
    -o-transition: all 0.25s ease;
    transition: all 0.25s ease;
}

.accordion_left .accordion .submenu .menu{border: none;}
.accordion_left .accordion .submenu .btn{
	padding: 4px 22px;
	border-radius: 2px;
	font-size: 12px;
    display: block;
    border: none; margin-left: auto;
    margin-right: auto;
    margin-top: 11px;
    margin-bottom: 11px;
    color: #eee;
    background-color: #026fd0;
    border-color: #111C2F;}
  .accordion_left .accordion .submenu .btn:focus,.accordion_left .accordion .submenu .btn:hover{
	background-color:#eee;
	outline: 0;
	color: #232E42;
}    
  
.accordion_left .accordion .submenu .form-group{ margin-bottom:0;}
.accordion_left .accordion .submenu .menu .checkbox-inline{color: #d9d9d9}
.accordion_left .accordion .submenu .menu a{
    display: block;
    text-decoration: none;
    color: #d9d9d9;
    padding: 6px;
    padding-left: 30px;
    -webkit-transition: all 0.25s ease;
    -o-transition: all 0.25s ease;
    transition: all 0.25s ease;}

.accordion_left .accordion .submenu .menu a:hover {
    background-color: rgba(5,40,245,0.40);
    color: #FFF;
}
.accordion_left .accordion .submenu .menu a:hover span{
    background-color:transparent;
    color: #FFF;
}
.accordion_left .accordion .submenu .menu a:hover img{
    background-color:transparent;
}

.accordion_left .accordion .submenu span{font-size: 12px;}
.accordion_left .accordion .submenu.menu img{vertical-align: middle;height: 14px;}

.input-group .form-control,.input-group .input-group-addon{background-color:transparent;border-radius:2px;border: 1px solid #003e64;}



.accordion_left .accordion .submenu li{background-color: transparent;border-bottom: 1px solid rgba(0,78,255,0.5);opacity: 0.8;}
.accordion_left .accordion .submenu .form-group input {background-color: transparent;border: 1px solid #003e64;padding-right: 0;}
.accordion_left .accordion .submenu input:focus{
	border: 1px solid #003e64;
	box-shadow: 0 0 8px #187adf;
	background-color:transparent;
	color:#d3d3d3;
}
.accordion_left .accordion .submenu .menu,.accordion_left .accordion .submenu .menu span{background-color: transparent}

.accordion_left .accordion .submenu .input-group{ padding-top: 10px;margin-left: auto; margin-right: auto;}
.accordion_left .accordion .submenu .input-group input{padding-right: 0}
.accordion_left .accordion .submenu .input-group span{padding-right: 10px }



/***************************选项卡切换**********************************/
.accordion_left .accordion .submenu .sp_menu
{
    text-align: center;
    bottom:0px;
    left:0px;
    width:100%;
    height:26px;
}
.accordion_left .accordion .submenu .sp_menu li{
    float:left;
    font-size:12px;
    color:#004eff;
    width:50%;
    padding-left:2%;
    height:26px;
    line-height:26px;
    cursor:pointer;
    display:block;

}
.accordion_left .accordion .submenu .sp_menu li:nth-child(1){
    border-right:rgba(0,78,255,0.5) 1px solid;
}

.accordion_left .accordion .submenu .sp_menu li:hover{
    color:#fff;
}
.accordion_left .accordion .submenu .sp_menu li.active{
    color:#fff;
    border-bottom:#004eff 2px solid;
}

