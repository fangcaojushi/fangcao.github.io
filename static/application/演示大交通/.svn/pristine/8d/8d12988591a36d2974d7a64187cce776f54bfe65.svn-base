.jwzqqy .jwzqqy_txt h4 {
	transition: all .5s;
	/*color: #02C9FF;*/
	cursor: default;
	font-size: 16px;
}

.jwzqqy .jwzqqy_txt {
	width: 60%;
	padding-left: 5%;
	height: 40px;
	overflow: hidden;
	float: left;
}

.jwzqqy:hover {
	/*background: rgba(0,64,184,.9);*/
}

.jwzqqy:hover .jwzqqy_txt h4 {
	transform: translateY(-8px);
}

.jwzqqy:hover .jwzqqy_txt p {
	transform: translateY(-18px);
}

.jwzqqy .jwzqqy_txt p {
	color: #fe9900;
	transform: translateY(100px);
	transition: all .5s;
}

.jwzqqy .jwzqqy_txt p i {
	color: #3185FF;
	font-style: normal;
	padding-left: 8px;
	cursor: pointer;
}


/************************************开关************************************************************************************/

.jwzqqy.cur .kaiguan-wrap {
	display: block;
}

.jwzqqy .kaiguan-wrap {
	width: 40px;
	height: 20px;
	margin: 0;
	cursor: pointer;
	float: right;
	margin: 10px 5px 0 0;
	/*display: none;*/
}

.jwzqqy .kaiguan-bg {
	width: 100%;
	height: 100%;
	background-color: #666;
	border-radius: 15px;
	padding: 3px;
	color: #fff;
	transition: all 0.2s ease;
}

.jwzqqy .kaiguan-switch {
	position: relative;
	left: 0px;
	width: 13px;
	height: 13px;
	border: solid 1px rgba(255, 255, 255, 1);
	background: rgba(255, 255, 255, .9);
	border-radius: 6px;
	transition: all 0.2s ease;
}

.jwzqqy .kaiguan-active .kaiguan-switch {
	left: 50%;
}

.jwzqqy .kaiguan-on,
.jwzqqy .kaiguan-off {
	position: absolute;
	transition: all 0.2s ease;
	padding-top: 15px;
	font-size: 2.4em;
	font-weight: bold;
}

.jwzqqy .kaiguan-on {
	margin-left: 80%;
}

.jwzqqy .kaiguan-off {
	margin-left: 10%;
	color: #dd2222;
}

.jwzqqy .kaiguan-active .kaiguan-off {
	color: #fff;
}

.jwzqqy .kaiguan-active .kaiguan-on {
	color: rgba(5, 40, 245, 0.70);
}

.jwzqqy .kaiguan-active .kaiguan-bg {
	background-color: #008000;
}

.bg7 h2 {
	margin-bottom: 0;
