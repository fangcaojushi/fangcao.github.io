.fensb {
		width: 100%;
		height: 160%;
		position: absolute;
		margin-top: 10%;
	}

.font_big{ font-size: 30px; color: #00FF00; padding-right: 10px; font-weight: bold; cursor: pointer; }

.shebei_sm {
	padding: 30px 0 0 10px;
}

.shebei_sm b{
	padding: 10px 20px 5px  20px;
	margin-top: 20px;
	font-size: 14px;
	border: 1px solid #02155b;
	border-radius: 5px;
	transition: all .5s;
	cursor: pointer;
}
.yydw {
		width: 100%;
		height: 150%;
		position: absolute;
		margin-top: 10%;
	}
.yydw tspan {
	font-size: 12px;
	border: 1px solid #02155b;
	border-radius: 5px;
	transition: all .5s;
	cursor: pointer;
	font-weight: bold;
}
.yydw .gsnr{
	font-size:10px;
	line-height:20px
}
.shebei_sm b:hover{
	background-color: #004EFF;
	color:#fff;
	 box-shadow: 0 0 5px #004EFF;
}
.shebei_sm table {
	position: absolute;
	bottom: 0px;
	width: 90%;
	text-align: center;
	color: #666;
	vertical-align: middle;
}

.shebei_sm table button {
	width: 80%;
	border: 0;
	vertical-align: super;
	font-weight: bold;
	font-size: 18px;
    background-color: transparent;
	border-radius: 3px;
	transform: all .5s;
}
.shebei_sm table button:hover,.font_big:hover {
	color: #fff;
	text-shadow: 0 0 15px #fff;
}
.baoxiu{ padding-left: 30px;}
@media only screen and (max-width:1440px) {
	.shebei_sm b{
		padding: 2px 20px;
		font-size: 11px;
		border: 1px solid #02155b;
	}
	.shebei_sm table {
		margin-top:10px;
		width: 90%;
		height: 40px;
		text-align: center;
		
	}
	.shebei_sm table button {
		width: 80%;
		border: 0;
		vertical-align: super;
		font-weight: bold;
		font-size: 14px;
	}
	.font_big{ font-size: 18px; color: #00FF00; padding-right: 10px; font-weight: bold;}
	.high_95 h5{margin-top: 0; margin-bottom: 5px;}
}