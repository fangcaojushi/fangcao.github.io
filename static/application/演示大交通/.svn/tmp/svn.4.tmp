@charset "UTF-8";

/* CSS Document */


/*****************************滚动数据**********************************************************/

.list_data {
	/*! border:#fff 1px solid; */
	height: 100%
}


/*.list_data:hover{ background:rgba(0,34,74,0.5); border:#22282e 1px solid; border-radius:10px;}*/

.list_data h5,
.dhbt_lvl {
	color: #3485ff;
	/*border-bottom:1px solid #aaa;*/
	font-weight: bold;
	position: relative;
	font-size: 16px;
}

.canvas_round .highcharts-title tspan {
	font-size: 16px
}

.dhbt_lvl em {
	font-size: 12px;
	color: #666;
	position: absolute;
	right: 10px;
	top: 15px;
	width: auto;
}

.list_data h5 a {
	position: absolute;
	right: 5px;
	color: #999;
	font-size: 12px;
	font-weight: normal;
	bottom: 3px;
}

.list_data .list_lh {
	min-height: 120px;
	height: 90%;
	overflow: hidden;
	margin-bottom: 20px;
}

.list_data .list_lh li {
	position: relative;
	padding: 3px;
	border-bottom: #374966 1px dashed
}


/*.list_data .list_lh li.lieven{ background:#f0f2f3;}
*/

.list_data .list_lh li p {
	line-height: 20px;
	margin: 0;
	padding: 3px 0;
	font-size: 12px
}

.list_data .list_lh li p a {
	color: #aabad5;
}

.list_data .list_lh li p em {
	width: 60%;
	text-align: center;
	font: normal 12px Arial;
	color: #99cc00;
	display: inline-block;
}

.list_data .list_lh li span {
	color: #999;
	padding-right: 8px;
	line-height: 22px;
}

.list_data .list_lh li p .btn_lh,
.list_data .list_lh li p .btn_rh {
	height: 15px;
	font-size: 11px;
	line-height: 15px;
	color: #fff;
	padding: 0px 5px;
	margin-top: 4px;
	border-radius: 2px;
	display: inline-block;
	cursor: pointer;
}

.list_data .list_lh li p .btn_lh {
	float: right;
}

.list_data .list_lh li p .btn_rh {
	margin-right: 5px;
}

.list_data .list_lh li p .btn_lh:hover {
	color: #fff;
	text-decoration: none;
}

.list_data .list_lh li p .btm p {
	font: normal 12px/24px 'Microsoft YaHei';
	text-align: center;
}

.progress {
	margin-bottom: 7%;
}

.basic_title number {
	color: #00FF00;
	font-weight: bold;
	font-size: 26px;
}


/*.ssjk-total-title,.progress{margin-top: 2%;}*/

.qjh {
	height: 230px
}

.qjzb {
	width: 100%;
}


/*全局指标*/

.qjzb td {
	width: 50%;
	height: 50%;
	text-align: center;
	cursor: default
}

.qjzb td:hover {
	background: #eee;
	color: #009900;
}

.fxzb {
	border: #b7fc90 1px solid;
	margin: 0px auto;
	clear: both;
	width: 100%;
	text-align: center;
}


/*分项指标*/

.fxzb td {
	border-right: #b7fc90 1px solid;
	border-bottom: #b7fc90 1px solid;
}

.fxzb tr i {
	display: block;
	font-size: 80%;
	color: #ade8ad;
}

.fxzb tr:nth-child(2n+1) {
	background: #edfce4;
}

.fxzb tr:nth-child(1) {
	background: #393;
	color: #fff;
	font-weight: bold
}

.highcharts-credits {
	display: none
}

.win_position4 .list_lh {
	height: 220px;
	overflow: hidden;
	width: 80%;
	margin: 20px auto;
}

.win_position4 .list_lh a {
	color: #99FF33;
}

.high_wai {
	height: 85%;
	position: relative;
}

.high_nei {
	height: 48%;
	position: relative;
}

.high_con {
	height: 100%;
	position: relative;
	overflow: hidden;
}

.canvas_round {
	min-width: 310px;
	height: 100%;
	margin: 0 auto
}

.rightbg1_c {
	padding: 0px;
	padding-left: 1px;
	width: 100%;
	height: 140px;
	overflow: hidden;
	padding-bottom: 20px;
}

.rightbg1_c img {
	position: absolute;
	z-index: 9;
	top: 0;
	left: 0;
	min-width: 200px;
	width: 100%;
	height: auto
}

.rightbg1_c_date {
	height: 30px;
	line-height: 30px;
	font-weight: bold;
	text-align: center;
	font-family: "Arial", "微软雅黑", "宋体";
	font-size: 15px;
	color: #fff;
	z-index: 99
}

.rightbg_jtzs {
	width: 100%;
	z-index: 99;
	position: absolute;
	left: 0;
	bottom: 0;
	height: auto
}

.rightbg_jtzs h1 {
	/*padding-bottom: 50px;*/
	text-align: center;
	line-height: 30px;
	font-family: "Arial", "微软雅黑", "宋体";
	font-size: 40px;
	color: #fff;
}

.rightbg_jtzs h2 {
	text-align: center;
	line-height: 20px;
	font-family: "微软雅黑", "Arial", "宋体";
	font-size: 14px;
	color: #fff;
}

#hot_pic {
	width: 100%;
	height: 100%;
}

#r-result {
	position: absolute;
	top: 50px;
	right: 0;
	width: 200px;
	height: 400px;
}


/*****************************历史数据**********************************************************/

.history_win {
	position: absolute;
	bottom: 0;
	left: 0;
	/* height:260px; */
	width: 100%;
	transition: bottom 300ms;
}

.history_win_hidden {
	bottom: -260px;
}

.history_win #bottom_win {
	width: 60px;
	height: 30px;
	/*z-index:9999; */
	position: absolute;
	top: -30px;
	right: 30px;
	cursor: pointer;
	border: 0;
}

.history_win .time_select {
	width: auto;
	height: 30px;
	z-index: 9999;
	position: absolute;
	top: -30px;
	left: 30px;
	cursor: pointer;
}

.history_win .time_select button {
	width: 120px;
	text-align: center;
	line-height: 30px;
	border: 0;
}

.zs_box {
	border: 1px solid rgba(219, 225, 248, 0.3);
	width: 100%;
	height: 93%;
	margin: 3% 0;
	position: relative;
}

.zs_box span {
	width: 25%;
	min-width: 120px;
	padding: 5px 0px;
	margin-left: 10px;
	margin-top: -10px;
	cursor: default;
	background-color: rgba(1, 9, 52, 1.0);
	display: block;
	text-align: center
}

.zs_box table {
	color: rgba(6, 248, 238, 1.00);
	border: 0;
	overflow-y: auto;
	height: 200px;
}

.zs_box .seblock {
	width: 100%;
	height: 10px;
	display: block;
	margin-top: 3px;
}

@media only screen and (max-width:1440px) {
	.list_data h5,
	.dhbt_lvl,
	.highcharts-title tspan {
		font-size: 14px;
	}
	/*.progress { margin-bottom: 7%;}*/
	.basic_title span {
		font-size: 9px;
	}
	.basic_title number {
		font-size: 18px;
	}
}


/*************************************交通运行态势********************************/

.ts_title {
	background: linear-gradient(to right, rgba(18, 84, 127, 0.4) 0%, rgba(18, 84, 127, 0) 100%);
	height: 26px;
	clear: both;
	overflow: hidden;
}

.ts_title h5 {
	margin: 0;
	line-height: 26px;
	font-weight: normal;
	margin-left: 12px;
}

.ts_title p {
	background: url(../images/xiangqing1.png) no-repeat;
	width: 9px;
	height: 9px;
	margin-top: 60%;
}

.ts_title p:hover,
.ts_title p:active {
	background: url(../images/xiangqing2.png) no-repeat;
}

.fmk {
	margin-bottom: 2%;
}

.jinchu {
	height: 10%;
	max-height: 12px;
	min-height: 5px;
	background-color: #0e192a;
}

.cheliang_title number {
	color: #00FF00;
	font-weight: bold;
	font-size: 24px;
}


