/*************************************持续拥堵时间********************************/

#clockyd {
	stroke: #000;
	stroke-linecap: round;
	fill: #008901;
}

#face {
	stroke-width: 1px;
}

#ticks {
	stroke-width: 1px;
}

#hands line {
	stroke-linejoin: bevel;
}

#hourhand {
	stroke-width: 4px;
}

#minutehand {
	stroke-width: 3px;
}

#numbers {
	font-size: 9px;
	text-anchor: middle;
	stroke: none;
	fill: #008901;
}

#yongdu {
	stroke: rgba(255, 0, 0, .9);
	stroke-width: 1px;
}

#ydzs {
	stroke: none
}

#ydzs .zs5 {
	fill: rgba(255, 0, 0, .8);
}

#ydzs .zs4 {
	fill: rgba(254, 153, 0, .8);
}

#ydzs .zs3 {
	fill: rgba(254, 209, 2, .8);
}

#ydzs .zs2 {
	fill: rgba(153, 204, 0, .8);
}

