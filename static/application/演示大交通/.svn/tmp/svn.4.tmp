	background: #039ffb
}

.datediv {
	width: 100%;
	height: 75px;
	overflow: hidden;
}

.datediv #nowDateTime {
	width: 50%;
	text-align: center;
}

.datediv iframe {
	margin-top: -8px;
}

.shuoming li {
	width: 90%;
	margin: 0 auto;
	display: block;
	border-bottom: #333 1px dotted;
	height: 25px;
}

.shuoming li * {
	cursor: default;
}

.shuoming li i {
	width: 13px;
	height: 100%;
	border-radius: 0px;
	margin: 0 10px;
	display: inline-block;
	vertical-align: middle;
